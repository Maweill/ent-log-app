; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Entertainment Log App"
run/main_scene="res://scenes/main/main.tscn"
config/features=PackedStringArray("4.1", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

UiManager="*res://scenes/autoload/ui_manager/ui_manager.tscn"
Panku="*res://addons/panku_console/console.tscn"

[display]

window/size/viewport_width=828
window/size/viewport_height=1792
window/stretch/mode="canvas_items"
window/handheld/orientation=5

[editor_plugins]

enabled=PackedStringArray("res://addons/kanban_tasks/plugin.cfg", "res://addons/panku_console/plugin.cfg", "res://addons/quick_script_change/plugin.cfg")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true
